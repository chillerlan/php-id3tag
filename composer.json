{
	"name": "chillerlan/php-id3tag",
	"description": "ID3 tag reader. PHP7.4+",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "stable",
	"keywords": [
		"id3", "mp3", "audio"
	],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-id3tag/issues",
		"source": "https://github.com/chillerlan/php-id3tag"
	},
	"require": {
		"php": "^7.4 || ^8.0",
		"ext-fileinfo": "*",
		"ext-json": "*",
		"ext-mbstring": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"phan/phan": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\ID3Tag\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\ID3TagTest\\": "tests/",
			"chillerlan\\ID3TagExamples\\": "examples/"
		}
	}
}
